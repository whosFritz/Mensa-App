{
  "workbench.colorTheme": "One Dark Pro",
  "files.autoSave": "afterDelay",
  "tailwindCSS.rootFontSize": 17,
  "http.proxyAuthorization": null,
  "workbench.iconTheme": "material-icon-theme",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "git.confirmSync": false,
  "cmake.configureOnOpen": true,
  "explorer.confirmDragAndDrop": false,
  "git.enableSmartCommit": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontWeight": "normal",
  "editor.fontLigatures": false,
  "editor.mouseWheelZoom": true,
  "terminal.integrated.fontSize": 14,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.productIconTheme": "fluent-icons",
  "explorer.confirmDelete": false,
  "gitlens.gitCommands.skipConfirmations": ["fetch:command", "switch:command"],
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "emmet.triggerExpansionOnTab": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "chatgpt.gpt3.apiKey": "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",
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.previewFlutterUiGuides": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "githubIssues.queries": [
    {
      "label": "Meine Probleme",
      "query": "default"
    },
    {
      "label": "Erstellte Probleme",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "Aktuelle Probleme",
      "query": "state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "dart.previewSdkDaps": null
}
